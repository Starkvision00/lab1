class NumberThread implements Runnable {
    public void run() {
        try {
            for (int n = 1; n <= 5; n++) {
                System.out.print("\nFactorial of " + n + " = ");
                Thread.sleep(1500);
            }
        } catch (InterruptedException e) {
            System.out.println("Number thread interrupted!");
        }
    }
}

class FactorialThread implements Runnable {
    public void run() {
        try {
            for (int n = 1; n <= 5; n++) {
                int fact = 1;
                for (int i = 1; i <= n; i++) fact *= i;
                System.out.print(fact + " ");
                Thread.sleep(1500);
            }
        } catch (InterruptedException e) {
            System.out.println("Factorial thread interrupted!");
        }
    }
}

public class MainThread {
    public static void main(String[] args) {
        new Thread(new NumberThread()).start();
        new Thread(new FactorialThread()).start();
    }
}
